---
import { Page, Block, Code } from '../../components/DocPage.js';
import * as info from '../../info.js';
---

<Page title="text">
  <Block>
    
      <Code slot="code" lang="js" code={`
import sheet, { mount } from '${info.cdnURL}';

mount(document.body, () => {
  const year = new Date().getFullYear();

  return sheet\`
    .year {
      text: \${year};
    }
  \`;
});
`} />
    </Block>
 
</Page>